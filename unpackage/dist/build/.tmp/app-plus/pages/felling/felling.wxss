
.feedback-submit {
	background-color: #0faeff;
	font-weight: 700;
	color: #fff;
	margin: 70rpx;
	opacity: 0.8;
	border-radius: 50rpx;
}
page {
	background-size: 100% 100%;
	overflow: hidden;
}
.btn1 {
	width: 80%;
	margin-top: 10%;
	margin-bottom: 10%;
	opacity: 0.7;
}
.feedback-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 20rpx;
	font-weight: 500;
	font-size: 35rpx;
	margin-top: 40rpx;
	color: #000;
}
.feedback-star-view.feedback-title {
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	margin: 0;
}
.feedback-input {
	width: 60%;
	font-weight: 400;
	font-size: 35rpx;
	height: 40rpx;
	padding: 10rpx 20rpx;
	line-height: 40rpx;
	background-color: #e1e1e1;
	border-radius: 30rpx;
	opacity: 0.8;
}
.list-item {
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 5px 0px;
	margin: 20rpx;
	background-color: #e1e1e1;
	text-align: center;
	opacity: 0.6;
	border-radius: 50rpx;
}
.list-text {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
._text {
	width: 40%;
	margin-left: 50rpx;
	font-size: 40rpx;
	color: #f7f7f7;
	font-weight: 700;
	text-align: center;
}

